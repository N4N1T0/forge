    "databases": [
        {
            "$id": "6889d4420039348bacac",
            "name": "forge-db",
            "enabled": true
        }
    ],
    "collections": [
        {
            "$id": "6889d472002c5a3a8edd",
            "$permissions": [
                "create(\"users\")",
                "read(\"users\")",
                "update(\"users\")",
                "delete(\"users\")"
            ],
            "databaseId": "6889d4420039348bacac",
            "name": "workspaces",
            "enabled": true,
            "documentSecurity": true,
            "attributes": [
                {
                    "key": "name",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 256,
                    "default": null,
                    "encrypt": false
                },
                {
                    "key": "userId",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 50,
                    "default": null,
                    "encrypt": false
                },
                {
                    "key": "icon",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 50,
                    "default": null,
                    "encrypt": false
                },
                {
                    "key": "slug",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 10,
                    "default": null,
                    "encrypt": false
                },
                {
                    "key": "theme",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "elements": [
                        "light",
                        "dark"
                    ],
                    "format": "enum",
                    "default": "dark"
                }
            ],
            "indexes": []
        },
        {
            "$id": "689222070020826336ec",
            "$permissions": [
                "create(\"users\")",
                "read(\"users\")",
                "update(\"users\")",
                "delete(\"users\")"
            ],
            "databaseId": "6889d4420039348bacac",
            "name": "Members",
            "enabled": true,
            "documentSecurity": false,
            "attributes": [
                {
                    "key": "userId",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 50,
                    "default": null,
                    "encrypt": false
                },
                {
                    "key": "workspaceId",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 50,
                    "default": null,
                    "encrypt": false
                },
                {
                    "key": "role",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "elements": [
                        "ADMIN",
                        "MEMBER"
                    ],
                    "format": "enum",
                    "default": null
                }
            ],
            "indexes": []
        }
    ],
    "buckets": [
        {
            "$id": "6889e227003dff29e499",
            "$permissions": [
                "create(\"users\")",
                "read(\"users\")",
                "update(\"users\")",
                "delete(\"users\")"
            ],
            "fileSecurity": false,
            "name": "images",
            "enabled": true,
            "maximumFileSize": 1000000,
            "allowedFileExtensions": [
                "jpg",
                "png",
                "svg",
                "jpeg"
            ],
            "compression": "none",
            "encryption": true,
            "antivirus": true
        }
    ]
}